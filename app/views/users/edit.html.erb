<%=simple_form_for @user do |f|%>
  <%=f.label :username%>
  <%=f.text_field :username, placeholder: :username%>
  <%=f.label :location%>
  <%=f.fields_for @user.location do |location|%>
    <%=location.label :address%>
    <%=location.text_field :address, value: @user.location.address%>
  <%end%>
<%end%>