<div class="main-page-grid">  
  <div class="main">
    <h2>Welcome, <%=@user.username%>.</h2>
    <%=link_to "Edit", edit_user_path(@user)%></span>
    <p>You are currently located in <%=user_location%>.</p><br><br>
    <h3>Check In</h3>
    <p>Where will you be skating today?</p>
    <%=form_for :check_in, url: user_check_ins_path(@user), :method => :post do |f|%>
      <%=f.label "Add location"%>
      <%=f.text_field :city%>
      <%=f.text_field :notes%>
      <%=f.submit%>
    <%end%>
    
  </div>
  <div>
    <h2>Your Spots</h2>
    <%=check_in%> <!--user helpers-->
  </div>
</div>