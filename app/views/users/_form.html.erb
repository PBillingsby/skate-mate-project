<%=form_for @user do |f|%>
  <%=render 'layouts/shared_errors', form_object: @user%>
  <h3><u>Sign Up</u></h3>
  <div class="form-group">
    <%=f.text_field :username, placeholder: :username, :autocomplete => 'off'%>
  </div>
  <div class="form-group">
    <%=f.text_field :email, placeholder: :email, :autocomplete => 'off'%>
  </div>
  <div class="form-group">
    <%=f.password_field :password, placeholder: :password%>
  </div>
  <div class="form-group">
    <%=f.password_field :password_confirmation, placeholder: :password_confirmation%>
  </div>
  <li><%=f.submit%></li>
<%end%>